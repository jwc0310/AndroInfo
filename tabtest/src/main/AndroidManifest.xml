<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.tabtest">

<application
    android:allowBackup="true"
    android:icon="@drawable/ic_launcher"
    android:label="@string/app_name"
    android:theme="@style/AppTheme" >
    <activity
        android:name=".MainActivity"
        android:label="@string/app_name" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name="com.test.tabtest.IconTabActivity" />
    <activity android:name="com.test.tabtest.SwipableIconTabActivity" />
    <activity android:name="com.test.tabtest.SwipableTextTabActivity" />
    <activity android:name="com.test.tabtest.TextTabActivity" />
</application>

</manifest>

